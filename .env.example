# Associated Press API Configuration
# Required: Your AP API key from api.ap.org
AP_API_KEY=your_ap_api_key_here

# Optional: API base URL (default: https://api.ap.org/media/v)
AP_BASE_URL=https://api.ap.org/media/v

# Optional: Request timeout in milliseconds (default: 30000)
AP_TIMEOUT=30000

# Optional: Number of retry attempts for failed requests (default: 3)
AP_RETRIES=3

# Optional: Enable debug logging (default: false)
AP_DEBUG=false

# Optional: Log level (default: info)
# Options: error, warn, info, debug
AP_LOG_LEVEL=info

# Optional: Enable request/response logging (default: false)
AP_VERBOSE_LOGGING=false